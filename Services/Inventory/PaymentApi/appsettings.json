{
  "LogOptions": {
    "Level": "information",
    "LogTemplate": "{Timestamp:HH:mm:ss} [{Level:u4}] {Message:lj}{NewLine}{Exception}",
    "Elastic": {
      "Enable": false,
      "ElasticServiceUrl": "http://localhost:9200"
    },
    "File": {
      "enable": true,
      "path": "logs/logs.txt",
      "interval": "day"
    }
  },
  "RabbitMq": {
    "HostName": "localhost",
    "ExchangeName": "InventoryExchange",
    "ExchangeType": "fanout",
    "RouteKey": "r1",
    "QueueName": "InventoryQueue",
    "IsLasyQueue": false,
    "AutoDelete": true,
    "UserName": "guest",
    "Password": "guest"
  },
  "AllowedHosts": "*",

  "ConnectionStrings": {
    "InventoryConnection": "Data Source=.;Initial Catalog=Inventory;TrustServerCertificate=True;Integrated Security=True"

  }


}
